{
  "name": "@integrations/microsoft-teams",
  "version": "0.1.0",
  "description": "Microsoft Teams integration for LLM Dev Ops platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "npm run test -- --testPathPattern=unit",
    "test:integration": "npm run test -- --testPathPattern=integration",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "microsoft",
    "teams",
    "bot",
    "webhook",
    "graph",
    "adaptive-cards"
  ],
  "license": "MIT"
}
