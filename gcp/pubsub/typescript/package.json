{
  "name": "@anthropic/gcp-pubsub",
  "version": "0.1.0",
  "description": "Google Cloud Pub/Sub integration for LLM DevOps platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "npm test -- --coverage"
  },
  "keywords": [
    "gcp",
    "pubsub",
    "google-cloud",
    "messaging",
    "events"
  ],
  "author": "Anthropic",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
