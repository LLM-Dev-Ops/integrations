{
  "name": "@integrations/azure-openai",
  "version": "0.1.0",
  "description": "Azure OpenAI Integration Module - A thin adapter layer for Azure OpenAI services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./services/chat": {
      "types": "./dist/services/chat/index.d.ts",
      "import": "./dist/services/chat/index.js"
    },
    "./services/embedding": {
      "types": "./dist/services/embedding/index.d.ts",
      "import": "./dist/services/embedding/index.js"
    },
    "./deployment": {
      "types": "./dist/deployment/index.d.ts",
      "import": "./dist/deployment/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js"
    },
    "./adapter": {
      "types": "./dist/adapter/index.d.ts",
      "import": "./dist/adapter/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "azure",
    "openai",
    "gpt-4",
    "embeddings",
    "ai",
    "llm",
    "chat",
    "streaming"
  ],
  "author": "Integrations Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "peerDependencies": {}
}
