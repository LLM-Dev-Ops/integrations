{
  "name": "@integrations/google-artifact-registry",
  "version": "1.0.0",
  "description": "Google Artifact Registry integration for LLM DevOps platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
