{
  "name": "@llm-devops/cloudflare-r2",
  "version": "0.1.0",
  "description": "Cloudflare R2 storage integration for LLM Dev Ops platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js"
    },
    "./objects": {
      "types": "./dist/objects/index.d.ts",
      "import": "./dist/objects/index.js"
    },
    "./multipart": {
      "types": "./dist/multipart/index.d.ts",
      "import": "./dist/multipart/index.js"
    },
    "./presign": {
      "types": "./dist/presign/index.d.ts",
      "import": "./dist/presign/index.js"
    },
    "./signing": {
      "types": "./dist/signing/index.d.ts",
      "import": "./dist/signing/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint src",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.3",
    "fast-xml-parser": "^4.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "prettier": "^3.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "cloudflare",
    "r2",
    "s3",
    "object-storage",
    "llm-devops"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/llm-devops/integrations"
  },
  "bugs": {
    "url": "https://github.com/llm-devops/integrations/issues"
  },
  "author": "LLM DevOps Team",
  "license": "MIT"
}
