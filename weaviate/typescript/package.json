{
  "name": "@llmdevops/weaviate-integration",
  "version": "0.1.0",
  "description": "Production-ready Weaviate Vector Database client for LLM DevOps platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.mjs",
      "require": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.mjs",
      "require": "./dist/auth/index.js",
      "types": "./dist/auth/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.mjs",
      "require": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.ts"
    },
    "./filter": {
      "import": "./dist/filter/index.mjs",
      "require": "./dist/filter/index.js",
      "types": "./dist/filter/index.d.ts"
    },
    "./search": {
      "import": "./dist/search/index.mjs",
      "require": "./dist/search/index.js",
      "types": "./dist/search/index.d.ts"
    },
    "./batch": {
      "import": "./dist/batch/index.mjs",
      "require": "./dist/batch/index.js",
      "types": "./dist/batch/index.d.ts"
    },
    "./aggregate": {
      "import": "./dist/aggregate/index.mjs",
      "require": "./dist/aggregate/index.js",
      "types": "./dist/aggregate/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema/index.mjs",
      "require": "./dist/schema/index.js",
      "types": "./dist/schema/index.d.ts"
    },
    "./observability": {
      "import": "./dist/observability/index.mjs",
      "require": "./dist/observability/index.js",
      "types": "./dist/observability/index.d.ts"
    },
    "./resilience": {
      "import": "./dist/resilience/index.mjs",
      "require": "./dist/resilience/index.js",
      "types": "./dist/resilience/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "weaviate",
    "vector-database",
    "semantic-search",
    "llm",
    "embeddings",
    "rag",
    "retrieval-augmented-generation",
    "hybrid-search",
    "similarity-search",
    "vector-search",
    "ai",
    "machine-learning",
    "database",
    "typescript",
    "graphql"
  ],
  "author": "LLM DevOps Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/llmdevops/integrations",
    "directory": "weaviate/typescript"
  },
  "bugs": {
    "url": "https://github.com/llmdevops/integrations/issues"
  },
  "homepage": "https://github.com/llmdevops/integrations/tree/main/weaviate/typescript#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
