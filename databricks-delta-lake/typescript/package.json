{
  "name": "@llm-devops/databricks-delta-lake",
  "version": "0.1.0",
  "description": "Production-ready Databricks Delta Lake client with resilience patterns, Unity Catalog support, and observability",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.mjs",
      "require": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.mjs",
      "require": "./dist/auth/index.js",
      "types": "./dist/auth/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.mjs",
      "require": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./http": {
      "import": "./dist/http/index.mjs",
      "require": "./dist/http/index.js",
      "types": "./dist/http/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "databricks",
    "delta-lake",
    "unity-catalog",
    "sql-warehouse",
    "llm",
    "ai",
    "data-engineering",
    "data-analytics",
    "apache-spark",
    "api-client",
    "typescript"
  ],
  "author": "LLM DevOps",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "msw": "^2.0.11",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/llm-devops/integrations.git",
    "directory": "databricks-delta-lake/typescript"
  },
  "bugs": {
    "url": "https://github.com/llm-devops/integrations/issues"
  },
  "homepage": "https://github.com/llm-devops/integrations/tree/main/databricks-delta-lake/typescript#readme"
}
