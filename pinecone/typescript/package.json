{
  "name": "@integrations/pinecone",
  "version": "0.1.0",
  "description": "Production-ready TypeScript client for the Pinecone Vector Database API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./types": {
      "types": "./dist/types/mod.d.ts",
      "import": "./dist/types/mod.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js"
    },
    "./transport": {
      "types": "./dist/transport/index.d.ts",
      "import": "./dist/transport/index.js"
    },
    "./operations": {
      "types": "./dist/operations/index.d.ts",
      "import": "./dist/operations/index.js"
    },
    "./batch": {
      "types": "./dist/batch/index.d.ts",
      "import": "./dist/batch/index.js"
    },
    "./validation": {
      "types": "./dist/validation/index.d.ts",
      "import": "./dist/validation/index.js"
    },
    "./namespace": {
      "types": "./dist/namespace/index.d.ts",
      "import": "./dist/namespace/index.js"
    },
    "./rag": {
      "types": "./dist/rag/index.d.ts",
      "import": "./dist/rag/index.js"
    },
    "./simulation": {
      "types": "./dist/simulation/index.d.ts",
      "import": "./dist/simulation/index.js"
    },
    "./metrics": {
      "types": "./dist/metrics/index.d.ts",
      "import": "./dist/metrics/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run rebuild && npm run test"
  },
  "keywords": [
    "pinecone",
    "vector-database",
    "embeddings",
    "similarity-search",
    "ai",
    "machine-learning",
    "rag",
    "retrieval"
  ],
  "author": "Integrations Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "peerDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/integrations/pinecone"
  }
}
